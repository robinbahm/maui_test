<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFrameworks>net7.0-ios;net7.0-android;net7.0-maccatalyst;net7.0-windows10.0.19041.0</TargetFrameworks>
    <UseMaui>true</UseMaui>
    <SingleProject>true</SingleProject>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <ApplicationTitle>DishLaundry</ApplicationTitle>
  </PropertyGroup>
  <ItemGroup>
    <MauiXaml Include="**/*.xaml" />
  </ItemGroup>
</Project>
